{
  "name": "a2ui-restaurant-finder-ts",
  "version": "0.1.0",
  "description": "Sample TypeScript Restaurant finder agent that uses a2ui UI and is hosted as an A2A server agent. Runs in V8 environment.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.7",
    "@google/adk": "^0.2.2",
    "@google/genai": "^1.35.0",
    "@hono/node-server": "^1.19.9",
    "ajv": "^8.17.1",
    "hono": "^4.11.4",
    "openai": "^6.16.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
